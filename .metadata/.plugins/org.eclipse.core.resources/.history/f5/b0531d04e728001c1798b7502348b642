package empSLLmenuderivven;

public class Main {

	public static void main(String[] args) {
		Employee e1= new Employee("a",1,200);
		Employee e2= new Employee("b",2,300);
		Employee e3= new Employee("c",3,400);
		Employee e4= new Employee("d",4,500);
		EmpList ll = new EmpList();
		ll.insertEmp(e1);
		ll.insertEmp(e2);
		ll.insertEmp(e3);
		ll.append(e4);
		System.out.println(ll);
	}

}
