package empSLLmenuderivven;

public class Node {
	private Node next;
	private Employee data;
	public Node getNext() {
		return next;
	}
	public void setNext(Node next) {
		this.next = next;
	}
	public Employee getData() {
		return data;
	}
	public void setData(Employee data) {
		this.data = data;
	}

	public String toString() {
		String str ="" + data;
		return str;
	}

	
}
